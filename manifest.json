{
  "name": "PUCMM WIFI Auto Login",
  "version": "0.4",
  "manifest_version": 2,  
  "description": "Esto es una extensión que permite iniciar sesión en la red WIFI de PUCMM Santiago de una manera más fácil y rápida.",
  "icons": {
      "128": "images/icons/icon128.png",
      "16": "images/icons/icon16.png",
      "32": "images/icons/icon32.png",
      "48": "images/icons/icon48.png"
   },
  "browser_action": {
      "default_icon": "images/icons_color/icon_grey.png",
      "default_title": "Click Para Conectar!"
   },
  "background": {"persistent": true, "page":"background.html"},
  
  "minimum_chrome_version": "18",

  "options_page": "options.html",
  "permissions": [ "tabs", "https://stwlan.priv/*"]

  }